{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}


<div class="content-wrapper">
   
    <!-- Main content -->
<div class="content">
	<div class="container-fluid">
		<div class="row">
			<div class="col-md-6">
				<div class="card card-info">
					<div class="card-header">
						Create Blog
						 {{ formActionLabel|default('app_customer')|trans ~ ' ' ~ title |trans }}
					</div><div class="card-body">

						{{ form_start(form) }}
						{{ form_widget(form) }}
						<div class="clearfix"></div>
                        <div class="form-group col-md-6 margin-t-10">
                            <input type="submit" class="btn btn-default" value="{{ 'Save'|trans }}"/>
                            <input type="reset" class="btn btn-default" value="{{ 'Reset'|trans }}"/>
                          
                        </div>
						{{ form_end(form) }}
				</div>
			</div>
		</div>
	</div>
</div>

{% endblock %}
